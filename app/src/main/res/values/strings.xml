    <!-- Main questionnaire -->
     <string name="question.preface">Madam, mister, you will be seen in consultation by a physician. Before starting the consultation, would you please answer this short questionnaire about the way you perceive your own health. The person accompanying you may help you</string>
    <string name="question.thanks">The survey is done. <b>Press submit to save your personal data on the tablet.</b></string>
    <string name="question.yes">Yes</string>
    <string name="question.no">No</string>
    <string name="question.male">Male</string>
    <string name="question.female">Female</string>
    <string name="question.0">ID = (3 letters of first name, 3 letters of last name, month of birth)</string>
    <string name="question.0.1">Please enter your first name?</string>
    <string name="question.0.2">Please enter your last name?</string>
    <string name="question.0.3">Please enter your month of birth?</string>
    <string name="question.1">1.</string>
    <string name="question.1.1">Have you involuntary lost weight in the past year?</string>
    <string name="question.1.2">If yes, was the weight loss more than 3 kg (6.6lb)?</string>
    <string name="question.2">2.</string>
    <string name="question.2.1">How many different drugs do you take daily?</string>
    <string name="question.3">3.</string>
    <string name="question.3.1">Do you have vision problems?</string>
    <string name="question.4">4.</string>
    <string name="question.4.1">Do you have hearing problems?</string>
    <string name="question.5">5.</string>
    <string name="question.5.1">Has someone around you noticed that you have memory problems?</string>
    <string name="question.6">6.</string>
    <string name="question.6.1">Do you receive help at home?</string>
    <string name="question.6.2">If yes, from whom (please select all applicable)</string>
    <string name="question.6.2.1">Family, and/or</string>
    <string name="question.6.2.2">Friend, and/or </string>
    <string name="question.6.2.3">Professional</string>

    <string name="question.7">7.</string>
    <string name="question.7.1">Do you need help for toileting?</string>
    <string name="question.8">8.</string>
    <string name="question.8.1">Do you need help for bathing?</string>
    <string name="question.9">9.</string>
    <string name="question.9.1">Do you need help for dressing?</string>
    <string name="question.10">10.</string>
    <string name="question.10.1">Do you need help for walking and/or transfering?</string>
    <string name="question.11">11.</string>
    <string name="question.11.1">Do you need help for feeding?</string>
    <string name="question.12">12.</string>
    <string name="question.12.1">Do you need help to use the phone?</string>
    <string name="question.13">13.</string>
    <string name="question.13.1">Do you need help for taking public transport?</string>
    <string name="question.14">14.</string>
    <string name="question.14.1">Do you need help for managing your own medications?</string>
    <string name="question.15">15.</string>
    <string name="question.15.1">Do you need help for handle your own finances?</string>
    <string name="question.16">16.</string>
    <string name="question.16.1">Are you incontinent (urine and/or stool)?</string>
    <string name="question.17">17.</string>
    <string name="question.17.1">How do you feel today?</string>
    <string name="question.17.1.1">Happy</string>
    <string name="question.17.1.2">Unhappy</string>
    <string name="question.17.1.3">Neither one nor the other</string>
    <string name="question.18">18.</string>
    <string name="question.18.1">Do you feel energetic?</string>
    <string name="question.19">19.</string>
    <string name="question.19.1">Did you do at least one hour per week of regular physical activity in the past month?</string>
    <string name="question.20">20.</string>
    <string name="question.20.1">Did you fall in the previous year (at least one fall)?</string>
    <!-- there is no number before next question -->
    <string name="question.21"></string>
    <string name="question.21.1">Have you received help to complete the questionnaire?</string>
